{
    "$schema": "shortcut.schema.json",
    "shortcuts": [
        {
            "name": "Windows open URL",
            "command": "cmd.exe /c start https://www.youtube.com/watch?v=dQw4w9WgXcQ",
            "keys": ["ctrl", "shift", "5"],
            "hidewindow": true
        },
        {
            "name": "macOS open terminal",
            "command": "/usr/bin/open -n https://www.youtube.com/watch?v=dQw4w9WgXcQ",
            "keys": ["ctrl", "shift", "n"]
        },
        {
            "name": "Linux open URL",
            "command": "/usr/bin/xdg-open https://www.youtube.com/watch?v=dQw4w9WgXcQ",
            "keys": ["ctrl", "shift", "e"]
        }
    ],
    "version": "0.0.1"
}
