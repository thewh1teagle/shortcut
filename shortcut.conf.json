{
    "$schema": "shortcut.schema.json",
    "shortcuts": [
        {
            "name": "Windows open URL",
            "command": "explorer.exe https://github.com/thewh1teagle/shortcut",
            "keys": ["ctrl", "shift", "5"]
        },
        {
            "name": "macOS open terminal",
            "command": "/usr/bin/open -a Terminal",
            "keys": ["ctrl", "shift", "n"]
        },
        {
            "name": "Linux open URL",
            "command": "/usr/bin/xdg-open https://github.com/thewh1teagle/shortcut",
            "keys": ["ctrl", "shift", "e"]
        }
    ],
    "version": "0.0.1"
}
